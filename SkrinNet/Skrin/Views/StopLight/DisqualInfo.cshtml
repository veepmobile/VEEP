@using serch_contra_bll.StopLightFreeEgrul.Info
@using Skrin.BLL.Infrastructure
@model DisqualInfo
@{
    Layout = null;
}
@foreach (DisqualInfoItem item in Model.Items)
{
    <table class="profile_table" cellspacing="0" cellpadding="0" border="0" style="width: 60%">
        <tr>
            <th style="width: 200px">Дата начала дисквалификации</th>
            <td>@item.StartDate.ToRusString()</td>
        </tr>
        <tr>
            <th>Дата окончания дисквалификации</th>
            <td>@item.EndDate.ToRusString()</td>
        </tr>
        <tr>
            <th>Дата вынесения судебным органом постановления о дисквалификации</th>
            <td>@item.CourtDate.ToRusString()</td>
        </tr>
        <tr>
            <th>Дата внесения записи в ЕГРЮЛ</th>
            <td>@item.GrnDate.ToRusString()</td>
        </tr>
        <tr>
            <th>Дата формирования сведений из ЕГРЮЛ</th>
            <td>@item.EgrulDate.ToRusString()</td>
        </tr>
    </table>
    <div style="clear: both; height: 10px;"></div>
}

