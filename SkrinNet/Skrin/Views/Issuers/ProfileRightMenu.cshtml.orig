@using Skrin.BLL.Infrastructure
@using Skrin.BLL.Authorization

@model Skrin.Models.Iss.Content.ProfileRightMenu
@{
    Layout = null;
    UserSession us = Context.GetUserSession();
    var _i = 0;
    var _j = 0;
}

<h5 style="text-align:right;">Дата проверки @DateTime.Now.AddDays(-1).ToRusString()</h5>
@if (us.UserId > 0)
{
    <div id="cloud_block">
        <div id="cloud_block_inner">
            <div id="cloud_container" data-id="@Model.issuer_id"></div>
            <div class="link_block" id="lb_3">
                <h3 class="cloud_switcher_header">
                    <span class="icon-doc-text"></span>Отчеты
                    <span class="icon-angle-down cloud_switcher_ar"></span>
                </h3>
                <table class="extra_table" border="0" cellpadding="0" cellspacing="0" id="rlist">
                    <tbody id="report_body">
                        <tr id="repact">
                            <td class="report_list" iss="@Model.ticker" onclick="reportlist_header_click(event)">
                                <div class="icon_block large">
                                    <span class="icon-download new ico"></span>
                                    <span>Сформировать</span>
                                </div>
                                <div class="dropmenu" style="display:none;" id="reports_@Model.ticker">
                                    <table class="droptable" cellspacing="0" cellpadding="0">
                                        <tr>
                                            <td class="droptd" align="left" valign="middle" style="text-decoration:none;">
                                                <div id="hrc" style="text-decoration:none;font-weight:bold;display:block;">
                                                    Выбор блоков в отчете
                                                </div>
                                                <ul class="pdfrepchoice">
                                                    @foreach (var cho in Model.choice)
                                                    {
                                                        <li class="pdfrepitems">
                                                            <div onclick="ToggleChB(this,event)" @((cho.disabled && !cho.selected) ? "style=\"color:#999;text-decoration:none;\"" : "")>
                                                                <input type="checkbox" class="rchoice" value="1" style="margin-right:5px;" @(cho.disabled ? "disabled" : "") @(cho.selected ? "checked" : "") />@cho.name
                                                            </div>
                                                        </li>
                                                    }
                                                </ul>
                                                <div style="text-align:center">
                                                    <input type="button" class="btns blue" value="Сформировать" onclick="@(ViewBag.CanCreateReport ? "GetPDFReport('" + @Model.ticker + "','" + @us.UserId + "')" : ViewBag.AccessFunction)" />
                                                </div>
                                            </td>
                                        </tr>

                                    </table>
                                </div>
                            </td>
                        </tr>
                        @{_i = 0;}
                        @if (Model.URep != null)
                        {

                            foreach (var u in Model.URep)
                            {
                                _i++;
                                <tr class="rep_arc" @(_i > 1 ? "style=display:none; val=1" : "") id="@("Z"+u.id)">
                                    <td class="repitem" style="height:38px">
                                        <div>
                                            <div class="icon_block">
                                                <span onclick="LoadReport('@u.id');">
                                                    <span class="icon-file-pdf pdf ico"></span>
                                                    <span>Отчет от @u.dt</span>

                                                </span>
                                                <span class="ddel" onclick="doDelReport('@u.id');">
                                                    <span class="ddel_1"><b>x</b>Удалить</span>
                                                </span>
                                            </div>
                                        </div>
                                    </td>
                                </tr>
                                if (_i > 1 && _i == Model.URep.Count)
                                {
                                    <tr class="rep_arc1">
                                        <td>
                                            <span class="icon-angle-down more_ico" id="report_togler" onclick="showReps()">Архив отчетов</span>
                                        </td>
                                    </tr>
                                }
                            }
                        }
                    </tbody>
                </table>
            </div>
            <div class="link_block" id="lb_4">
                <h3 class="cloud_switcher_header">
                    <span class="icon-th-large"></span>Состоит в группах
                    <span class="icon-angle-down cloud_switcher_ar"></span>
                </h3>
                <table class="extra_table" border="0" cellpadding="0" cellspacing="0">
                    <tr id="addgroup_row">
                        <td>
                            <div class="icon_block large" onclick="@(ViewBag.CanAddToGroup ? "AddToGroup('" + @Model.issuer_id + "')" : ViewBag.AccessFunction)">
                                <span class="icon-plus new ico"></span>
                                <span>Добавить</span>
                            </div>
                        </td>
                    </tr>
                </table>
            </div>
        </div>

    </div>

}

<div id="uncloud_block">
@if (Model.egrul_is_possible)
        {
            <div class="link_block">
                <h3>Выписка из ЕГРЮЛ</h3>
                <table class="extra_table" border="0" cellpadding="0" cellspacing="0">
                    <tbody id="egrul_body">
                        @if (Model.egrul_is_black)
                        {
                            <tr>
                                <td>Информация по данному ЮЛ отсутствует в ЕГРЮЛ.</td>
                            </tr>
                        }
                        else
                        {
                            <tr>
                                <td>
                                    <div class="egrul_list" iss="@Model.ticker">
                                        @if (Model.EGRULPDFisToday)
                                        {


                                            if (Model.EGRULPDFis_pdf == "1")
                                            {
                                                <a href="@string.Format("/Report/EgrulPdf/{0}/{1}", Model.EGRULPDFdate, Model.ogrn)" target="_blank">
                                                    <span class="icon-file-pdf pdf ico" id="top_egrul"></span>
                                                    <span>Выписка из ЕГРЮЛ от @Model.EGRULPDFdate_string</span>
                                                </a>
                                            }
                                            else
                                            {
                                                <a href="#" onclick="@("getRTF('/Report/Egrul/" + Model.ogrn+ "');")">
                                                    <span class="icon-file-word word ico"></span>
                                                    <span>Выписка из ЕГРЮЛ от @Model.EGRULPDFdate_string </span>
                                                </a>
                                            }
                                        }
                                        else
                                        {
                                            <div id="top_egrul" @(us.UserId == 0 ? Html.Raw("class=\"icon_block large disable\" onclick=\"" + ViewBag.LoginFunction + "\"") : Html.Raw("class=\"icon_block large\"  onclick=\"checkEgrulPDF(event,'" + Model.ogrn + "')\""))>
                                                <span class="icon-download new ico"></span>
                                                <span>Сформировать актуальную</span>
                                            </div>
                                    <!--span onclick="@("complite_job('EgrulPDF_" + Model.ogrn + "', '<a href=\\'/Report/EgrulPdf/20160101/123456789012\\'><span class=\\'icon-file-pdf pdf ico\\' id=\\'top_egrul\\'></span><span>Выписка из ЕГРЮЛ от 03.02.2016</span></a>');")">Уже сформировал...</span-->
                                            }
                                        </div>
                                    </td>
                                </tr>
                            <!-- || (Model.EGRULPDFisToday) -->
                            if (Model.EGRULPDF != null)
                            {
                                foreach (var u in Model.EGRULPDF)
                                {
                                    _j++;
                                    <tr class="rep_arc" @(_j > 1  ? "style=display:none; val=1" : "") id="@("ep"+_j)">
                                        <td class="repitem" style="height:38px">
                                            <div>
                                                @if (u.is_pdf == "1")
                                                {

                                                    <a href="@string.Format("/Report/EgrulPdf/{0}/{1}", u.datestring, u.ogrn)" target="_blank">
                                                        <span class="icon-file-pdf pdf ico"></span>
                                                        <span>Выписка из ЕГРЮЛ от @u.dt </span>
                                                    </a>
                                                }
                                                else
                                                {
                                                    <a href="#" onclick="@("getRTF('/Report/Egrul/" + Model.ogrn+ "');")">
                                                        <span class="icon-file-word word ico"></span>
                                                        <span>Выписка из ЕГРЮЛ от @u.dt </span>
                                                    </a>
                                                }
                                            </div>
                                        </td>
                                    </tr>

                                }
                                <tr class="rep_arc1" id="rep_switch" @((_j > 1 && _j == Model.EGRULPDF.Count) ? "" : "style=display:none;")>
                                    <td>
                                        <span class="more_ico icon-angle-down" id="report_toglerE" onclick="showEReps()"> Развернуть архив</span>
                                    </td>
                                </tr>

                            }
                        }
                    </tbody>
                </table>
            </div>
        }
