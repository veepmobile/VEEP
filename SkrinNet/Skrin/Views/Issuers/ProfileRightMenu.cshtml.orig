                        <td>
                            <div class="icon_block med">
                                <span class="icon-newspaper link ico"></span>
                                <h4 onclick="gotomenu(26)">Филиалы и представительства</h4>
                                @if (ViewBag.OpenProfile) {
                                <div class="link_block_info">
                                    <span class="explain">
