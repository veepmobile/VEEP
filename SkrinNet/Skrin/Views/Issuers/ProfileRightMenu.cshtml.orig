                                }
                                        @if (!Model.zak_data.participiant_cnt.IsNullOrDefault())
                                        {
                                            @:В качестве участника: @Model.zak_data.participiant_cnt.NullableToString()<br />  (на сумму: @Model.zak_data.participiant_sum.ConvertTextSum())
                                }
                                    </span>
                                </div>
                                }
                            </div>
                        </td>
                    </tr>
                }
                @if (Model.pravo_data != null)
