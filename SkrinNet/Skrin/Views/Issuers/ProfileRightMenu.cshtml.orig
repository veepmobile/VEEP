                                }
                                    </span>
                                </div>
                                }
                            </div>
                        </td>
                    </tr>
                }
                @if (!Model.subsids_count.IsNullOrDefault())
                {
                    <tr>
                        <td>
                            <div class="icon_block med">
                                <span class="icon-newspaper link ico"></span>
                                <h4 onclick="gotomenu(25)">Дочерние и зависимые общества</h4>
